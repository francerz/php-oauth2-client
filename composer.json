{
    "name": "francerz/oauth2-client",
    "description": "Client side OAuth 2.0 Authorization Framework library",
    "keywords": ["oauth2", "psr-7"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Francerz\\OAuth2\\Client\\" : "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Francerz\\OAuth2\\Client\\Dev\\" : "dev"
        }
    },
    "require": {
        "psr/http-message": "^1.0",
        "psr/http-client": "^1.0",
        "francerz/oauth2-core": "^0.3",
        "francerz/php-power-data": "^0.1.0"

    },
    "require-dev": {
        "francerz/http": "^0.3",
        "phpunit/phpunit": "^8.5"
    }
}
